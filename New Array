#include <iostream>
#include "bits/stdc++.h"
using namespace std;
void print (long long a[] , long long b[] , long long n) {
long long c[n * 2];
for ( int i = 0; i<n *2; i++) {
if (i<n) {
c[i] = b[i];
}
else {
c[i]=a[i%n];
}
}
for (int i = 0 ; i < n*2 ; i++) {
    cout << c[i] << ' ' ;
}
};

int main() {
long long n ;
cin >> n ;
long long a[n] , b[n] ;
for (int i = 0 ; i < n ; i++) {
    cin >> a[i] ;
}
for (int i = 0 ; i < n ; i++) {
    cin >> b[i] ;
}
print (a,b,n) ;
}
