#include <iostream>
#include "bits/stdc++.h"
using namespace std;
void prime (long long n) {
    bool f = 1 ;
    if (n<2) {
        cout << "No" << endl;
    } else {
    for (int i = 2 ; i <= sqrt(n) ; i++) {
        if (n % i == 0) {
            f = 0;
            break;
        }
    }
    }
    if (f) cout << "YES" << endl ;
    else cout << "NO" << endl;
}
bool prime2(long long n) {
    bool f=1 ;
    if (n<2) return false;
    for (int i = 2 ; i <= sqrt(n) ; i++) {
        if (n%i==0) {
            f=0 ;
            break;
        }
    }
    return f ;
}
int main() {
int t ;
cin >> t ;
while (t--) {
    long long n;
    cin >> n;
    if (prime2(n)) {
        cout << "YES" << endl;
    } else cout << "NO" << endl;
}
}
