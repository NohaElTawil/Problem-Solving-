#include <iostream>
#include <string>

using namespace std;

int main() {
    string s;
    getline(cin, s);

    int freq[26] = {0};

    for (int i = 0; i < s.size(); ++i) {
        char ch = s[i];
        if (ch >= 'a' && ch <= 'z') {
            freq[ch - 'a']++;
        }
    }

    int counter = 0;
    for (int i = 0; i < 26; ++i) {
        if (freq[i] > 0)
            counter++;
    }

    cout << counter << endl;
    return 0;
}
