#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    cin >> s;

    int count = 0;
    int n = s.length();

    for (int i = 0; i < n; ++i) {
        if (s[i] != 'Q') continue;
        for (int j = i + 1; j < n; ++j) {
            if (s[j] != 'A') continue;
            for (int k = j + 1; k < n; ++k) {
                if (s[k] == 'Q') {
                    count++;
                }
            }
        }
    }

    cout << count << "\n";
    return 0;
}
