#include <iostream>
#include <string>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    string a, b;
    cin >> a >> b;

    if (a == b) {
        cout << -1 << "\n";
    } else {
        cout << max((int)a.size(), (int)b.size()) << "\n";
    }

    return 0;
}
