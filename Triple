#include <iostream>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t; cin >> t;
    while (t--) {
        int n; cin >> n;
        int* freq = new int[n+1]();  
        int* a = new int[n];
        for (int i = 0; i < n; i++) {
            cin >> a[i];
            freq[a[i]]++;
        }

        int ans = -1;
        for (int i = 1; i <= n; i++) {
            if (freq[i] >= 3) {
                ans = i;
                break;
            }
        }

        cout << ans << "\n";

        delete[] a;
        delete[] freq;
    }

    return 0;
}
