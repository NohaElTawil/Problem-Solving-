#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);
    vector<int> prefix(n);

    for (int i = 0; i < n; i++) {
        cin >> arr[i];
        if (i == 0)
            prefix[i] = arr[i];
        else
            prefix[i] = prefix[i - 1] + arr[i];
    }

    int m;
    cin >> m;

    while (m--) {
        int j;
        cin >> j;

        int start = 0, end = n - 1, ans = -1;
        while (start <= end) {
            int mid = (start + end) / 2;
            if (prefix[mid] >= j) {
                ans = mid;
                end = mid - 1;
            } else {
                start = mid + 1;
            }
        }

        cout << (ans + 1) << endl;
    }

    return 0;
}
